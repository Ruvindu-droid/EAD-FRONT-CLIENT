<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@drawable/station01"
tools:context=".Authentication">


    <TextView
        android:id="@+id/RegisterLable"
        android:layout_width="249dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="143dp"
        android:layout_marginTop="202dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="467dp"
        android:text="Login Potral"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/Sbjinp"
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="293dp"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="374dp"
        android:background="@drawable/rkinputs"
        android:hint="Please Enter Your User Name"
        android:inputType="textPersonName"
        android:padding="14dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="15sp"
        android:textStyle="italic" />

    <EditText
        android:id="@+id/msginp"
        android:layout_width="wrap_content"
        android:layout_height="64dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="377dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="290dp"
        android:background="@drawable/rkinputs"
        android:hint="Please Enter Your Password"
        android:inputType="textPassword"
        android:padding="14dp"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="15sp"
        android:textStyle="italic" />

    <Button
        android:id="@+id/register"

        android:layout_width="wrap_content"
        android:layout_height="77dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="35dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="548dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="106dp"
        android:background="@drawable/fund_border_red"
        android:onClick="GoRegAct"
        android:padding="10dp"
        android:text="Back"
        android:textAllCaps="false"
        android:textColor="#FE2B0A"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="RtlCompat" />

    <Button
        android:id="@+id/sendtea"

        android:layout_width="wrap_content"
        android:layout_height="67dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="465dp"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="199dp"
        android:background="@drawable/fund_border_red"
        android:onClick="signin"
        android:padding="10dp"
        android:text="Authorize me"
        android:textAllCaps="false"
        android:textColor="#FE2B0A"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:ignore="RtlCompat" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="149dp"
        android:layout_height="136dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="128dp"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="134dp"
        android:layout_marginBottom="549dp"
        app:srcCompat="@drawable/test3" />


</RelativeLayout>



