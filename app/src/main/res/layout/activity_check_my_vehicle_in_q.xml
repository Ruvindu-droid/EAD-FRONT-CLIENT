<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fund_background"
    android:orientation="vertical"
    tools:context=".CheckMyVehicleInQ">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/fund_background">


        <TextView
            android:id="@+id/textView6"
            android:layout_width="128dp"
            android:layout_height="20dp"
            android:text="Your vehicle Type :"
            android:textColor="#403C3C"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.127"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mapView2"
            app:layout_constraintVertical_bias="0.278" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="98dp"
            android:layout_height="23dp"
            android:text="Arrival Time :"
            android:textColor="#403C3C"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.115"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mapView2"
            app:layout_constraintVertical_bias="0.381" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="318dp"
            android:layout_height="26dp"
            android:text="If you were waited here, but unable to fill,"
            android:textColor="#403C3C"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.387"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mapView2"
            app:layout_constraintVertical_bias="0.464" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="320dp"
            android:layout_height="32dp"
            android:text="Filled From the Station and Get Off your Vehicle,"
            android:textColor="#403C3C"
            android:textSize="15sp"
            android:textStyle="italic"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.417"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mapView2"
            app:layout_constraintVertical_bias="0.642" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="72dp"
            android:text="Check my Vehicle in Fual Queue"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="372dp"
            android:layout_height="112dp"
            app:layout_constraintBottom_toTopOf="@+id/textView7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.487"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.514"
            app:srcCompat="@drawable/account_box" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="264dp"
            android:layout_height="24dp"
            android:text="Please Eneter Your Vehicle Number :"
            android:textColor="#322E2E"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.863"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.145" />

        <Button
            android:id="@+id/textView4"
            android:layout_width="216dp"
            android:layout_height="40dp"
            android:background="@drawable/fund_border_blue"
            android:onClick="LoadDetails"
            android:text="Check My Vehicle Status"
            android:textColor="#322E2E"
            android:textSize="10sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.184"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.299" />

        <Button
            android:id="@+id/textView11"
            android:layout_width="171dp"
            android:layout_height="40dp"
            android:background="@drawable/fund_border_blue"
            android:onClick="sendToInitialLanding"
            android:text="Back to Home"
            android:textColor="#322E2E"
            android:textSize="10sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.816"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.924" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="122dp"
            android:layout_height="29dp"
            android:text="In the station of :"
            android:textColor="#403C3C"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.124"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mapView2"
            app:layout_constraintVertical_bias="0.184" />

        <TextView
            android:id="@+id/amount6"
            android:layout_width="194dp"
            android:layout_height="30dp"
            android:layout_marginStart="6dp"
            android:layout_marginEnd="5dp"
            android:background="@drawable/fund_border_blue"
            android:padding="10dp"
            android:textColor="#686161"
            android:textColorHint="#686161"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.849"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.383"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/amount8"
            android:layout_width="193dp"
            android:layout_height="31dp"
            android:layout_marginStart="6dp"
            android:layout_marginEnd="5dp"
            android:background="@drawable/fund_border_blue"
            android:padding="10dp"
            android:textColor="#686161"
            android:textColorHint="#686161"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.845"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.46"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/amount9"
            android:layout_width="193dp"
            android:layout_height="31dp"
            android:layout_marginStart="6dp"
            android:layout_marginEnd="5dp"
            android:background="@drawable/fund_border_blue"
            android:textStyle="bold"
            android:padding="10dp"
            android:textColor="#686161"
            android:textColorHint="#686161"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.85"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.537"
            tools:ignore="SpeakableTextPresentCheck" />

        <EditText
            android:id="@+id/amount7"
            android:layout_width="228dp"
            android:layout_height="30dp"
            android:layout_marginStart="6dp"
            android:layout_marginEnd="5dp"
            android:background="@drawable/fund_border_blue"
            android:inputType="text"
            android:padding="10dp"
            android:textColor="#686161"
            android:textColorHint="#686161"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.72"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.201"
            tools:ignore="SpeakableTextPresentCheck" />

        <Button
            android:id="@+id/AddtoQButton"
            android:layout_width="321dp"
            android:layout_height="43dp"
            android:layout_marginStart="42dp"
            android:layout_marginEnd="42dp"
            android:layout_marginBottom="136dp"
            android:background="@drawable/fund_border_red"
            android:onClick="RemoveMyVehicle"
            android:text="Filled Successfully, Taking Off"
            android:textColor="#1515F0"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/addtoQButton"
            android:layout_width="325dp"
            android:layout_height="40dp"
            android:layout_marginStart="42dp"
            android:layout_marginEnd="42dp"
            android:layout_marginBottom="232dp"
            android:background="@drawable/fund_border_red"
            android:onClick="RemoveMyVehicle"
            android:text="Unable to Fill, Taking OFF"
            android:textColor="#1515F0"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/mapView2"
            android:layout_width="64dp"
            android:layout_height="68dp"
            android:layout_marginBottom="560dp"
            android:background="@drawable/car3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.121"
            app:layout_constraintStart_toStartOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</LinearLayout>

