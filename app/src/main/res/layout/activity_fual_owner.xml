<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fund_background"
    android:orientation="vertical"
    tools:context=".FualOwner">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/fund_background">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="258dp"
            android:layout_height="45dp"
            android:text="Manage your station Details.Please use the following,"
            android:textColor="#403C3C"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"

            app:layout_constraintHorizontal_bias="0.228"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mapView2"
            app:layout_constraintVertical_bias="0.527" />


        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="148dp"
            android:text="Station Potral"
            android:textColor="#FFFFFF"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/textView5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />
        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="88dp"
            android:text="Welcomming User!"
            android:textColor="#403C3C"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.074"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />




        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="396dp"
            android:layout_height="94dp"
            android:layout_marginTop="4dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView1"
            app:srcCompat="@drawable/account_box" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Account :"
            android:textColor="#322E2E"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView2"
            app:layout_constraintEnd_toEndOf="@+id/imageView4"
            app:layout_constraintHorizontal_bias="0.246"
            app:layout_constraintStart_toStartOf="@+id/imageView4" />


        <TextView
            android:id="@+id/textView2"
            android:layout_width="158dp"
            android:layout_height="21dp"
            android:layout_marginTop="12dp"
            android:text="user Account"
            android:textColor="#322E2E"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/imageView4"
            app:layout_constraintHorizontal_bias="0.204"
            app:layout_constraintStart_toEndOf="@+id/textView3"
            app:layout_constraintTop_toTopOf="@+id/imageView4" />

        <Button
            android:id="@+id/textView4"
            android:layout_width="115dp"
            android:layout_height="35dp"
            android:layout_marginStart="80dp"
            android:layout_marginTop="12dp"
            android:onClick="sendToInitialLanding"
            android:background="@drawable/fund_border_blue"
            android:text="Log Out"
            android:textColor="#322E2E"
            android:textSize="10sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@+id/imageView4"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="Manage your Fual Station Here"
            android:textColor="#403C3C"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.532"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView4" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="284dp"
            android:layout_height="46dp"
            android:text="For Registering your new/another Fual Station with details,"
            android:textColor="#403C3C"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.23"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mapView2"
            app:layout_constraintVertical_bias="0.109" />

        <Button
            android:id="@+id/button"
            android:layout_width="313dp"
            android:layout_height="42dp"
            android:layout_marginStart="42dp"
            android:layout_marginEnd="42dp"
            android:onClick="sendToRegisterStation"
            android:layout_marginBottom="192dp"
            android:background="@drawable/fund_border_blue"
            android:text="Add My Fual Station"
            android:textColor="#FE2B0A"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/button2"
            android:layout_width="313dp"
            android:layout_height="42dp"
            android:layout_marginStart="42dp"
            android:layout_marginEnd="42dp"
            android:onClick="sendToUpdateStationStatus"
            android:layout_marginBottom="76dp"
            android:background="@drawable/fund_border_blue"
            android:text="Manage My Fual Station"
            android:textColor="#FE2B0A"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/mapView2"
            android:layout_width="184dp"
            android:layout_height="130dp"
            android:layout_marginBottom="320dp"
            android:background="@drawable/test5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>




</LinearLayout>

